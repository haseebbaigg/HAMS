<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="12dp"
    android:gravity="center_vertical"
    android:background="@color/colorPrimary"
    >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="ClickMenu"
        android:src="@drawable/ic_baseline_menu_244">
    </ImageView>

    <ImageView
        android:layout_width="115dp"
        android:layout_height="32dp"
        android:layout_marginLeft="50px"
        android:src="@drawable/logomaker1"
        android:textAlignment="center"
        android:textStyle="bold"></ImageView>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="100px"
        android:onClick="ClickOption"
        android:layout_marginLeft="500px"
        android:src="@drawable/ic_baseline_more_vert_2444">
    </ImageView>


</LinearLayout>